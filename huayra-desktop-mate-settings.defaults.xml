<mateconfentryfile>
  <entrylist base="/apps/gksu">
    <entry>
      <key>sudo-mode</key>
      <value>
        <int>0</int>
      </value>
    </entry>
  </entrylist>
  <entrylist base="/desktop/mate">
    <entry>
      <key>background/picture_filename</key>
      <value>
        <string>/usr/share/images/huayra-desktop/huayra-wallpaper.png</string>
      </value>
    </entry>
    <entry>
      <key>background/picture_options</key>
      <value>
        <string>zoom</string>
      </value>
    </entry>
    <entry>
      <key>interface/gtk_theme</key>
      <value>
        <string>HuayraVioleta</string>
      </value>
    </entry>
    <entry>
      <key>interface/icon_theme</key>
      <value>
        <string>huayra</string>
      </value>
    </entry>
  </entrylist>
  <entrylist base="/desktop/mate/accesibility">
    <entry>
      <key>keyboard/enable</key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
  </entrylist>

  <entrylist base="/apps/caja">
    <entry>
      <key>desktop/computer_icon_visible</key>
      <value>
        <bool>false</bool>
      </value>
    </entry>
  </entrylist>
  <entrylist base="/apps/marco">
    <entry>
      <key>global_keybindings/run_command_terminal</key>
      <value>
        <string>&lt;Primary&gt;&lt;Alt&gt;T"</string>
      </value>
    </entry>
    <entry>
      <key>general/num_workspaces</key>
      <value>
        <int>1</int>
      </value>
    </entry>
    <entry>
      <key>general/button_layout</key>
      <value>
        <string>:minimize,maximize,close</string>
      </value>
    </entry>
    <entry>
      <key>general/theme</key>
      <value>
        <string>HuayraVioleta</string>
      </value>
    </entry>
  </entrylist>
  <entrylist base="/apps/mate-notification-daemon">
    <entry>
      <key>popup_location</key>
      <value>
        <string>botom_right</string>
      </value>
    </entry>
    <entry>
      <key>theme</key>
      <value>
        <string>slider</string>
      </value>
    </entry>
  </entrylist>
  <entrylist base="/apps/mate-terminal">
    <entry>
      <key>global/use_menu_accelerators</key>
      <value>
        <bool>false</bool>
      </value>
    </entry>
  </entrylist>
  <entrylist base="/apps/mate-terminal/profiles/Default">
    <entry>
      <key>background_type</key>
      <value>
        <string>transparent</string>
      </value>
    </entry>
    <entry>
      <key>background_color</key>
      <value>
        <string>#2B9D2B9D2B9D</string>
      </value>
    </entry>
    <entry>
      <key>background_darkness</key>
      <value>
        <float>0.92</float>
      </value>
    </entry>
    <entry>
      <key>foreground_color</key>
      <value>
        <string>#FFFFFFFFFFFF</string>
      </value>
    </entry>
    <entry>
      <key>use_theme_colors</key>
      <value>
        <bool>false</bool>
      </value>
    </entry>
  </entrylist>
  <entrylist base="/apps/panel/default_setup/objects/">
    <entry>
      <key>object_menu/object_type</key>
      <value>
        <string>menu-object</string>
      </value>
    </entry>
    <entry>
      <key>object_menu/toplevel_id</key>
      <value>
        <string>top_panel</string>
      </value>
    </entry>
    <entry>
      <key>object_menu/position</key>
      <value>
        <int>1</int>
      </value>
    </entry>
    <entry>
      <key>object_menu/tooltip</key>
      <value>
        <string>Huayra</string>
      </value>
    </entry>
  </entrylist>
  <entrylist base="/apps/panel/default_setup/applets/">
    <entry>
      <key>show_desktop_button/toplevel_id</key>
      <value>
        <string>top_panel</string>
      </value>
    </entry>
    <entry>
      <key>show_desktop_button/position</key>
      <value>
        <int>2</int>
      </value>
    </entry>
    <entry>
      <key>window_menu/position</key>
      <value>
        <int>5</int>
      </value>
    </entry>
    <entry>
      <key>window_menu/locked</key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
    <entry>
      <key>window_menu/toplevel_id</key>
      <value>
        <string>top_panel</string>
      </value>
    </entry>
    <entry>
      <key>window_menu/panel_right_stick</key>
      <value>
        <bool>false</bool>
      </value>
    </entry>
    <!-- clock -->
    <entry>
      <key>clock/position</key>
      <value>
        <int>400</int>
      </value>
    </entry>
    <entry>
      <key>clock/locked</key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
    <entry>
      <key>clock/panel_right_stick</key>
      <value>
        <bool>false</bool>
      </value>
    </entry>
    <!-- area de notificacion -->
    <entry>
      <key>notification_area/position</key>
      <value>
        <int>30</int>
      </value>
    </entry>
    <entry>
      <key>notification_area/locked</key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
    <entry>
      <key>notification_area/panel_right_stick</key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
    <!-- applet mixer -->
    <entry>
      <key>applet_mixer/object_type</key>
      <value>
        <string>external-applet</string>
      </value>
    </entry>
    <entry>
      <key>applet_mixer/applet_iid</key>
      <value>
        <string>MixerAppletFactory::MixerApplet</string>
      </value>
    </entry>
    <entry>
      <key>applet_mixer/position</key>
      <value>
        <int>20</int>
      </value>
    </entry>
    <entry>
      <key>applet_mixer/locked</key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
    <entry>
      <key>applet_mixer/panel_right_stick</key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
    <entry>
      <key>applet_mixer/toplevel_id</key>
      <value>
        <string>top_panel</string>
      </value>
    </entry>
  </entrylist>
  <!-- object access (acesibilidad)-->
  <entrylist base="/apps/panel/default_setup/objects/object_access">
    <entry>
      <key>object_type</key>
      <value>
        <string>menu-object</string>
      </value>
    </entry>
    <entry>
      <key>menu_path</key>
      <value>
        <string>mate-applications:/Universal Access/</string>
      </value>
    </entry>
    <entry>
      <key>tooltip</key>
      <value>
        <string>Accesibilidad</string>
      </value>
    </entry>
    <entry>
      <key>toplevel_id</key>
      <value>
        <string>top_panel</string>
      </value>
    </entry>
    <entry>
      <key>locked</key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
    <entry>
      <key>position</key>
      <value>
        <int>10</int>
      </value>
    </entry>
    <entry>
      <key>panel_right_stick</key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
    <entry>
      <key>use_menu_path</key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
  </entrylist>
  <!-- object object_control_center -->
  <entrylist base="/apps/panel/default_setup/objects/object_control_center">
    <entry>
      <key>object_type</key>
      <value>
        <string>launcher-object</string>
      </value>
    </entry>
      <entry>
      <key>launcher_location</key>
      <value>
        <string>mate-control-center.desktop</string>
      </value>
    </entry>
    <entry>
      <key>toplevel_id</key>
      <value>
        <string>top_panel</string>
      </value>
    </entry>
    <entry>
      <key>position</key>
      <value>
        <int>5</int>
      </value>
    </entry>
    <entry>
      <key>panel_right_stick</key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
  </entrylist>
  <entrylist base="/apps/panel/default_setup/objects/object_shutdown">
    <entry>
      <key>object_type</key>
      <value>
        <string>action-applet</string>
      </value>
    </entry>
    <entry>
      <key>action_type</key>
      <value>
        <string>shutdown</string>
      </value>
    </entry>
    <entry>
      <key>toplevel_id</key>
      <value>
        <string>top_panel</string>
      </value>
    </entry>
    <entry>
      <key>position</key>
      <value>
        <int>0</int>
      </value>
    </entry>
    <entry>
      <key>locked</key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
    <entry>
      <key>panel_right_stick</key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
  </entrylist>
  <!-- applet window_list (panel inferior) -->
  <entrylist base="/apps/panel/default_setup/applets/window_list/">
    <entry>
      <key>toplevel_id</key>
      <value>
        <string>bottom_panel</string>
      </value>
    </entry>
    <entry>
      <key>position</key>
      <value>
        <int>0</int>
      </value>
    </entry>
    <entry>
      <key>locked</key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
    <entry>
      <key>panel_right_stick</key>
      <value>
        <bool>false</bool>
      </value>
    </entry>
  </entrylist>
  <!-- sacar desktop switcher del panel de abajo -->
  <entrylist base="/apps/panel/default_setup/applets/workspace_switcher">
    <entry>
      <key>toplevel_id</key>
      <value>
        <string></string>
      </value>
    </entry>
  </entrylist>
  <!-- settings del top_panel -->
  <entrylist base="/apps/panel/default_setup/general">
    <entry>
      <key>applet_id_list</key>
      <value>
        <list type="string">
          <value>
            <string>show_desktop_button</string>
          </value>
          <value>
            <string>clock</string>
          </value>
          <value>
            <string>window_menu</string>
          </value>
          <value>
            <string>notification_area</string>
          </value>
          <value>
            <string>applet_mixer</string>
          </value>
          <value>
            <string>window_list</string>
          </value>
        </list>
      </value>
    </entry>
    <entry>
      <key>object_id_list</key>
      <value>
        <list type="string">
          <value>
            <string>object_menu</string>
          </value>
          <value>
            <string>object_access</string>
          </value>
          <value>
            <string>object_control_center</string>
          </value>
          <value>
            <string>object_shutdown</string>
          </value>
        </list>
      </value>
    </entry>
  </entrylist>
  <entrylist base="/desktop/mate/peripherals/touchpad">
    <entry>
      <key>tap_to_click</key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
  </entrylist>
  <!-- screensaver -->
  <entrylist base="/apps/mate-screensaver">
    <entry>
      <key>mode</key>
      <value>
        <string>single</string>
      </value>
    </entry>  
    <entry>
      <key>themes</key>
      <value>
        <list type="string">
          <value>
            <string>screensavers-huayra-mate-flying-cow</string>
          </value>
        </list>
      </value>
    </entry>
  </entrylist>
</mateconfentryfile>
