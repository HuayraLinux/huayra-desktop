#! /bin/sh
set -e

#DEBHELPER#

if [ "$1" = "configure" ]; then
  # Se migro la configuracion a ../huayra-desktop-mate-settings.defaults.xml
  MATECONF_CONFIG_SOURCE=`mateconftool-2 --get-default-source`
  mateconftool-2 --config-source="${MATECONF_CONFIG_SOURCE}" --direct --load /etc/huayra/huayra-desktop-mate-settings.defaults.xml >/dev/null

  exit 0

fi

